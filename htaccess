<IfModule mod_rewrite.c>

# Enable Rewrite Engine
RewriteEngine On
RewriteBase /

# Force clients to use https
RewriteCond %{HTTPS} !=on
RewriteRule (.*) https://www.domain.com/$1 [R=301,L]

</IfModule>